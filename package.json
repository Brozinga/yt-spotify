  },
  "homepage": "./",
  "main": "public/electron.js",
  "build": {
    "appId": "com.brozinga.yt-spotify",
    "productName": "MyApp",
    "copyright": "Copyright Â© 2019 Seu Nome",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "name": "reactron",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.19.2",
    "cross-env": "^7.0.2",
    "electron-is-dev": "^1.2.0",
    "node-notifier": "^6.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-electron-titlebar": "^1.0.9",
    "react-electron-web-view": "^2.0.1",
    "react-icons": "^3.9.0",
    "react-player": "^1.15.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron public/electron.js\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-build": "yarn build",
    "electron-build": "electron-builder -mwl"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rescripts/cli": "^0.0.14",
    "@rescripts/rescript-env": "^0.0.12",
    "concurrently": "^5.1.0",
    "electron": "^18.3.7",
    "electron-builder": "^22.4.1",
    "typescript": "^3.8.3",
    "wait-on": "^4.0.1"
  }
}
